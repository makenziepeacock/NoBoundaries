    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "interview-with-bebe-miller-2018-3-of-4--canvas-1-gesel-mason-annotation-1.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Keep doing this project because I'm like ... I don't feel at all like it was ... Yes, I'm doing these solos but it doesn't feel like it's about me at all.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": {
          "id": "https://makenziepeacock.github.io/NoBoundaries/interview-with-bebe-miller-2018-3-of-4-/canvas-1/canvas",
